{
  "name": "caqti-async-ppx",
  "version": "0.0.3",
  "esy": {
    "build": [
      [
        "dune",
        "build",
        "-p",
        "ppx_rapper"
      ]
    ],
    "buildDev": "refmterr dune build  @all",
    "buildsInSource": "_build",
    "buildEnv": {
      "ODOC_SYNTAX": "re"
    },
    "release": {
      "bin":[],
      "includePackages": [
        "root",
        "ocaml",
         "@opam/pg_query",
         "@opam/caqti",
         "@opam/ppxlib",
          "@opam/base",
           "@opam/caqti-async"
      ]
    }
  },
  "dependencies": {
    "@esy-ocaml/reason": ">= 3.4.0",
    "@opam/pg_query": "*",
    "@opam/ppxlib": "*",
    "@opam/base": "*",
    "@opam/caqti": "*",
    "@opam/caqti-async": "*",
    "ocaml": "~4.8.0",
    "refmterr": "*"
  },
  "devDependencies": {
    "@opam/merlin": "*",
    "ocaml": "~4.8.0",
    "@opam/odoc": "*",
    "@opam/ocaml-lsp-server": "ocaml/ocaml-lsp:ocaml-lsp-server.opam"
  }
}